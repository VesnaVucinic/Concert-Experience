<h1>Concerts Attended</h1>
 
<table>
 
  <tbody>
    <% @concerts.map do |c|%>
      <tr>
        <td><%= c.artist.name %></td>
        <td><%= c.venue %></td>
        <td><%= c.city %></td>
        <td><%= c.state %></td>
        <td><%= c.date %></td>
        <td><%= link_to "New Experience", new_concert_experience_path(c.id)%>
        <td><%= link_to "Your Experience", experience_path(c.id) %>
      
        <td><%= link_to "Experiences", concert_experiences_path(c.id) %>
      </tr>
    <% end %>
  </tbody>
</table>
 
